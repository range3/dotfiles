# set aliases for frequently used commands
alias ec='exec code'
alias ecode='exec code'

# hexdump
alias hexdump1='od -t x1 -Ax'
alias hexdump2='od -t x2 -Ax'
alias hexdump4='od -t x4 -Ax'

# typo
alias sl='ls'
alias scd='cd'
alias svi='vi'

# for ls
alias ll='ls -lF'
alias la='ls -A'
alias lla='ls -alF'
alias l='ls -CF'

# bookmarks
alias goto='CDPATH"$HOME/.bookmarks" cd -P'
